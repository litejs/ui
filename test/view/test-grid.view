@view test-grid public
	@css
		.border-white, .border-white > .col, .border-white > .group > .col {
			border-color: #fff;
		}
	anim-page
		h2 Main in the middle grid
		.grid.gap2.border-white
			.col.lg-6 main
			.col.md-6.lg-3 A
			.col.md-6.lg-3 B
			.group
				.col.sm-6 C
				.col.sm-6 D
			.col E
		h2 Test golden grid
		.grid.gap2.border-white
			.col.sm-g2 c1
			.col.sm-g3 c2
			.col.sm-g1.md-g2 c3
			.col.sm-g1.md-g3 c4
			.col.sm-g1.md-g2 c5
			.col.sm-g1.md-g4 c6
			.col.sm-g1.md-g5 c7
			.col.sm-g1.md-g2.lg-g3 c8
			.col.sm-g1.md-g3.lg-g5 c9
			.col.sm-g1.md-g2.lg-g6 c10
			.col.sm-g1.md-g4.lg-g4 c11
			.col.sm-g1.md-g5.lg-g5 c12
		h2 Test tiles
		.grid.gap2.border-white
			&css: "height", "300px"
			.col.sm-4.group.h2of3
				.col.h1of2 c1
				.col.h1of2 c2
			.col.sm-8.h2of3 c3
			.col.sm-8.h1of3 c6
			.col.sm-4.h1of3 c7



